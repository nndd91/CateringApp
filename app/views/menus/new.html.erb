<%= form_for @menu do |f| %>

  <%= f.label :item%>
  <%= f.text_field :item%>

  <%= f.label :itemtag%>
  <%= f.text_field :itemtag%>

  <%= f.label :description%>
  <%= f.text_field :description%>


  <%= f.label :img_location%>
  <%= f.file_field :img_location%>

  <%= f.submit %>
  <%= link_to 'Back to index', root_path %>

<% end %>
